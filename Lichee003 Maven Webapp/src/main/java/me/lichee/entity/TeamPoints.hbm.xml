<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="me.lichee.entity">

    <class name="TeamPoints" table="team_points">
        <id name="id" column="points_id">
        	<generator class="native"/>
        </id>
         <property name="number" column="points_number" not-null="false"/>
         
   		<many-to-one name="team" class="me.lichee.entity.Team" fetch="select" lazy="false">  
        	<column name="team_id" />   
		</many-to-one>  
   		
    </class>
    
	<!--
		 <many-to-one name="user" 
        class="tgh.check.model.User" 
        column="user_id"
        lazy="false"
        not-null="true"/> 
	 -->
</hibernate-mapping>